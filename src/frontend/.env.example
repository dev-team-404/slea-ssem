# Frontend Environment Configuration
# Copy this file to .env and update values for your local environment

# API Mode: Set to 'false' to use real backend, 'true' to use mock
VITE_MOCK_SSO=true
VITE_MOCK_API=false

# API Base URL (optional, defaults to '/api' which uses Vite proxy)
# For production: VITE_API_BASE_URL=https://api.yourdomain.com
# For development with Vite proxy: leave commented (uses '/api')
# VITE_API_BASE_URL=http://localhost:8000
